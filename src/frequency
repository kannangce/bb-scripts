#!/usr/bin/env bb

(require
   '[clojure.tools.cli :refer [parse-opts]])

(def progname "fequencies")

(def cli-options
  [["-f" "--file" "File to process"]
   ["-s" "--sort" "Sort column"]
   ["-d" "--descending" "Sort in descending order"]
   ["-h" "--help"]])



(defn format
  [data options]
  (reduce #(str %1 "\t"  %2 "\n") result))

(defn get-data
[options])

